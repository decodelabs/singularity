{
    "name": "decodelabs/singularity",
    "description": "Consolidated URI parsing and resolving",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/collections": "^0.11.3",
        "decodelabs/compass": "^0.3.1",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/nuance": "^0.1.2",

        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Singularity\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
